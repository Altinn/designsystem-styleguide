  </div>
  <section id="content" tabindex="-1">
    <div class="container">
      <div class="row">
        <div class="col-sm-12 col-lg-10 offset-lg-1 mb-4">
          {{# articlestart }} {{> organismer-artikkel-start }} {{/ articlestart }}
        </div>
      </div>
      <div class="row">
        <div class="col-sm-12 col-lg-12 mb-5">
          {{> molekyler-sammenligning-header }} {{# first-compare }} {{> organismer-sammenligning }} {{/ first-compare }} {{# second-compare }} {{> organismer-sammenligning }} {{/ second-compare }} {{# third-compare }} {{> organismer-sammenligning }} {{/ third-compare
          }}
        </div>
        
      </div>
      
      <div class="row">
        <div class="col-sm-12 col-lg-6 mb-2">
          {{# first-card-icon }} {{> molekyler-ikon-link }} {{/ first-card-icon }}
        </div>
        
        <div class="col-sm-12 col-lg-6 mb-2">
          {{# second-card-icon }} {{> molekyler-ikon-link }} {{/ second-card-icon }}
        </div>
        
      </div>
      
    </div>
    
  </section>
  {{> organismer-footer }}
